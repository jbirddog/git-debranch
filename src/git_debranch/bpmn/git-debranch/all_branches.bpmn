        <spiffworkflow:preScript>spawn_args_map = {
    "merged": ["git", "branch", "--merged"],
    "unmerged": ["git", "branch", "--no-merged"],
    "remote": ["git", "branch", "--remotes"],
    "current": ["git", "branch", "--show-current"],
}</spiffworkflow:preScript>
