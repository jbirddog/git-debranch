    <bpmn:scriptTask id="Activity_1rwssw3" name="Extract results">
      <bpmn:extensionElements>
        <spiffworkflow:unitTests>
          <spiffworkflow:unitTest id="happiest_of_paths">
            <spiffworkflow:inputJson>{
  "merged_branches_result": {
    "returncode": 0,
    "stdout": " a\n b\n c\n\n",
    "stderr": ""
  },
  "unmerged_branches_result": {
    "returncode": 0,
    "stdout": " d\n e\n f\n\n",
    "stderr": ""
  },
  "remote_branches_result": {
    "returncode": 0,
    "stdout": " g\n h\n i\n\n",
    "stderr": ""
  },
  "current_branch_result": {
    "returncode": 0,
    "stdout": "j\n",
    "stderr": ""
  }
}</spiffworkflow:inputJson>
            <spiffworkflow:expectedOutputJson>{
  "result": { "returncode": 0 },
  "current": "j\n",
  "merged": " a\n b\n c\n\n",
  "remote": " g\n h\n i\n\n",
  "unmerged": " d\n e\n f\n\n"
}</spiffworkflow:expectedOutputJson>
          </spiffworkflow:unitTest>
        </spiffworkflow:unitTests>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_0plnctp</bpmn:incoming>
      <bpmn:outgoing>Flow_04ggq0k</bpmn:outgoing>
      <bpmn:script># pre-conditions of this script, would love to move to pre script
# or have contract support
assert merged_branches_result["returncode"] == 0
assert unmerged_branches_result["returncode"] == 0
assert remote_branches_result["returncode"] == 0
assert current_branch_result["returncode"] == 0

result = { "returncode": 0 }

merged = merged_branches_result["stdout"]
unmerged = unmerged_branches_result["stdout"]
remote = remote_branches_result["stdout"]
current = current_branch_result["stdout"]

del(merged_branches_result)
del(unmerged_branches_result)
del(remote_branches_result)
del(current_branch_result)
del(spawn_args)</bpmn:script>
    </bpmn:scriptTask>
